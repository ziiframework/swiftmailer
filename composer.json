{
    "name": "ziiframework/swiftmailer",
    "description": "zii-framework",
    "keywords": ["zii-framework", "swiftmailer"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/ziiframework/swiftmailer/issues",
        "source": "https://github.com/ziiframework/swiftmailer"
    },
    "authors": [
        {
            "name": "Yii Team",
            "email": "team@yiiframework.com",
            "homepage": "http://www.yiiframework.com/team",
            "role": "Founder and project lead"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "ziiframework/zii": "3.0.x-dev",
        "swiftmailer/swiftmailer": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.cn"
        }
    ],
    "autoload": {
        "psr-4": { "yii\\swiftmailer\\": "src" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
